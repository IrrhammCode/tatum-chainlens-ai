{
  "name": "tatum-chainlens",
  "version": "1.0.0",
  "description": "Tatum ChainLens - Your Complete Blockchain Analytics Platform",
  "main": "server.js",
  "scripts": {
    "start": "node server-simple.js",
    "start:bot": "node telegram-bot.js",
    "start:both": "concurrently \"npm start\" \"npm run start:bot\"",
    "dev": "nodemon server-simple.js",
    "docker": "docker-compose up --build",
    "mcp": "npx @tatumio/blockchain-mcp",
    "setup": "node setup.js",
    "test-api": "node test-api.js",
    "test-wallet": "node test-wallet.js",
    "test-fallback": "curl -X POST http://localhost:3000/api/force-fallback",
    "test-multichain": "curl -X POST http://localhost:3000/api/test-multichain -H 'Content-Type: application/json' -d '{\"address\":\"0xd8dA6BF26964aF9D7eEd9e03E53415D37aA96045\",\"type\":\"portfolio\"}'",
    "status": "curl http://localhost:3000/api/status"
  },
  "keywords": [
    "tatum-chainlens",
    "tatum",
    "blockchain",
    "analytics",
    "mcp",
    "ai-chat",
    "multi-wallet",
    "nft-gallery",
    "portfolio-tracker",
    "hackathon",
    "defi",
    "nft",
    "wallet-analysis",
    "multi-chain",
    "fallback-system"
  ],
  "author": "Tatum ChainLens Team - MCP Hackathon 2025",
  "license": "MIT",
  "dependencies": {
    "@tatumio/blockchain-mcp": "^1.0.5",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "axios": "^1.6.0",
    "dotenv": "^16.3.1",
    "node-telegram-bot-api": "^0.64.0",
    "concurrently": "^8.2.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}